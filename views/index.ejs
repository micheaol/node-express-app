 <%- include("./partials/head.ejs") %> 
 
<body>
    <%- include('./partials/nav.ejs') %> 
    
    <div class="blog-container">
        <h2>All Blogs </h2>
        <% if (blogs.length > 0) { %>
             <% blogs.forEach(blog => { %>
                <a href="/blogs/<%= blog._id %> ">
                    <h3><%= blog.title %> </h3>
                </a>
                    <p><%= blog.snipet %> </p>
             <% }); %> 
        <% }else{%>
                <h4>There are no blogs to show</h4>
       <% } %>
    </div>
  
    
</body>
</html>